{
  "name": "@mytrader/backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsup",
    "dev": "node scripts/dev.mjs",
    "start": "node scripts/start.mjs",
    "typecheck": "pnpm exec tsc --noEmit -p tsconfig.json",
    "phase-r2:gate-snapshot": "node scripts/phase-r2-gate-snapshot.mjs",
    "verify:fts5": "pnpm run build && node scripts/verify-fts5.mjs",
    "verify:insights-e2e": "pnpm run build && node dist/verify-insights-e2e.js",
    "verify:position-engine": "pnpm run build && node dist/verify-position-engine.js",
    "verify:completeness-v2": "pnpm run build && node dist/verify-completeness-v2.js"
  },
  "dependencies": {
    "@mytrader/shared": "workspace:*",
    "@duckdb/duckdb-wasm": "^1.28.1",
    "sql.js": "^1.11.0"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "tsup": "^8.0.0"
  }
}
